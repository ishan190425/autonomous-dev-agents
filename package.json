{
  "name": "autonomous-dev-agents",
  "version": "1.0.0-alpha",
  "private": true,
  "description": "Ship software with autonomous AI dev teams",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/core && npm run build -w packages/cli && npm run build -w apps/web",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "test:coverage": "npm run test:coverage --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "dev": "npm run dev --workspace=packages/cli",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "agents",
    "autonomous",
    "development",
    "multi-agent",
    "devtools",
    "cli"
  ],
  "author": "Rathi Industries",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishan190425/autonomous-dev-agents.git"
  },
  "homepage": "https://github.com/ishan190425/autonomous-dev-agents#readme",
  "bugs": {
    "url": "https://github.com/ishan190425/autonomous-dev-agents/issues"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
